<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<context:component-scan base-package="com.bridgelabz.annotationautowired"></context:component-scan>
   <context:annotation-config/>
   <!-- bean definitions go here -->
	<bean id="student" class="com.bridgelabz.annotationautowired.Student">
	<property name="name" value="Vishal"></property>
	<property name="sId" value="17"></property>
	</bean>
	<bean id="student1" class="com.bridgelabz.annotationautowired.StudentAutoWired">
	</bean>
</beans>